{
    "compilerOptions": {
        "target": "ES6",
        // "module": "ES6",
        // "moduleResolution": "Classic",
        "lib": ["dom", "DOM"],
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "strict": false,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "esModuleInterop": true
    },
    "include": ["frontend/**/*.js", "node_modules/**/*.js"],
    "exclude": []
}
