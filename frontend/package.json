{
  "name": "@plutojl/client",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript API for programmatically interacting with Pluto notebooks",
  "type": "module",
  "main": "dist/standalone/index.js",
  "module": "dist/standalone/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:js && npm run build:types",
    "build:js": "rollup -c",
    "build:types": "mkdir -p dist/standalone && cp standalone/index.d.ts dist/standalone/index.d.ts && echo 'export * from \"./standalone/index\";' > dist/index.d.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "pluto",
    "julia",
    "notebook",
    "api",
    "typescript"
  ],
  "author": "Pluto.jl Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fonsp/Pluto.jl.git",
    "directory": "frontend/api"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^20.11.0",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "immer": "^10.0.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.esm.js"
    }
  }
}